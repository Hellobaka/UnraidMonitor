<UserControl
    x:Class="me.cqp.luohuaming.UnraidMonitor.UI.Views.CommandConfigPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="clr-namespace:me.cqp.luohuaming.UnraidMonitor.UI.Views.Controls"
    xmlns:hc="https://handyorg.github.io/handycontrol"
    xmlns:local="clr-namespace:me.cqp.luohuaming.UnraidMonitor.UI.Views">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="2*" />
            <ColumnDefinition Width="5*" />
        </Grid.ColumnDefinitions>
        <controls:CommandListControl Grid.Column="0" DataContext="{Binding ListVM}" />
        <controls:CommandDetailControl Grid.Column="1" DataContext="{Binding DetailVM}" />
    </Grid>
</UserControl>